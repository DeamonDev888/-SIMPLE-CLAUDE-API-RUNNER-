#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Deamon/Desktop/Backup/Serveur MCP/Workflow/node_modules/.pnpm/fastmcp@3.26.8_hono@4.11.3/node_modules/fastmcp/dist/bin/node_modules:/mnt/c/Users/Deamon/Desktop/Backup/Serveur MCP/Workflow/node_modules/.pnpm/fastmcp@3.26.8_hono@4.11.3/node_modules/fastmcp/dist/node_modules:/mnt/c/Users/Deamon/Desktop/Backup/Serveur MCP/Workflow/node_modules/.pnpm/fastmcp@3.26.8_hono@4.11.3/node_modules/fastmcp/node_modules:/mnt/c/Users/Deamon/Desktop/Backup/Serveur MCP/Workflow/node_modules/.pnpm/fastmcp@3.26.8_hono@4.11.3/node_modules:/mnt/c/Users/Deamon/Desktop/Backup/Serveur MCP/Workflow/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Deamon/Desktop/Backup/Serveur MCP/Workflow/node_modules/.pnpm/fastmcp@3.26.8_hono@4.11.3/node_modules/fastmcp/dist/bin/node_modules:/mnt/c/Users/Deamon/Desktop/Backup/Serveur MCP/Workflow/node_modules/.pnpm/fastmcp@3.26.8_hono@4.11.3/node_modules/fastmcp/dist/node_modules:/mnt/c/Users/Deamon/Desktop/Backup/Serveur MCP/Workflow/node_modules/.pnpm/fastmcp@3.26.8_hono@4.11.3/node_modules/fastmcp/node_modules:/mnt/c/Users/Deamon/Desktop/Backup/Serveur MCP/Workflow/node_modules/.pnpm/fastmcp@3.26.8_hono@4.11.3/node_modules:/mnt/c/Users/Deamon/Desktop/Backup/Serveur MCP/Workflow/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/fastmcp@3.26.8_hono@4.11.3/node_modules/fastmcp/dist/bin/fastmcp.js" "$@"
else
  exec node  "$basedir/../.pnpm/fastmcp@3.26.8_hono@4.11.3/node_modules/fastmcp/dist/bin/fastmcp.js" "$@"
fi
